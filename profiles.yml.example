# Copy this file to ~/.dbt/profiles.yml and update as needed
# Or set DBT_PROFILES_DIR to this directory

safestreets:
  target: dev
  outputs:
    dev:
      type: duckdb
      path: 'safestreets.duckdb'
      threads: 4

